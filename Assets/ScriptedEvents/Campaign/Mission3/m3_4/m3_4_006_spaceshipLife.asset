  cutsceneDialogue:
    dialogueBoxes:
    - dialogueText: Huh?
      nameTag: Anna
      portrait: {fileID: 0}
    - dialogueText: Can you tell me what it's like living here?
      nameTag: Pepper
      portrait: {fileID: 0}
    - dialogueText: You'll find out soon enough, won't you?
      nameTag: Anna
      portrait: {fileID: 0}
    - dialogueText: Yeah, but part of why I wanted to come up here was to find out,
